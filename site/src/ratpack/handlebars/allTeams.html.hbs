{{> partials/header.html}}
{{> partials/navigation.html}}
{{> partials/sidebar.html}}
{{> partials/topbar.html}}
<!-- Begin: Content -->
<section id="content" class="table-layout animated fadeIn">
    <!-- begin: .tray-center -->
    <div class="tray tray-center">
        <body>
        <div class="panel">
            <div class="panel-body pn">
                <div class="table-responsive">
                    <table class="table admin-form theme-warning tc-checkbox-1 fs13">
                        <thead>
                        <tr class="bg-light">
                            <th class="text-center hidden-sm hidden-xs">Select</th>
                            <th class="hidden-sm hidden-xs">Image</th>
                            <th class="">Team Name</th>
                            <th class="hidden-sm hidden-xs">Goals</th>
                            <th class="">Key Player</th>
                            <th class="text-right hidden-sm hidden-xs">Status</th>

                        </tr>
                        </thead>
                        <tbody>
                        {{#each model}}
                            <tr>
                                <td class="text-center hidden-sm hidden-xs">
                                    <label class="option block mn">
                                        <input type="checkbox" name="mobileos" value="FR">
                                        <span class="checkbox mn"></span>
                                    </label>
                                </td>
                                <td class="w100 hidden-sm hidden-xs">
                                    <img class="img-responsive mw40 ib mr10" title="user"
                                         src="assets/icon.jpg">
                                </td>
                                <td class="">{{team.name}}</td>
                                <td class="hidden-sm hidden-xs">33</td>
                                <td class="">Connor Pasty</td>
                                <td class="text-right hidden-sm hidden-xs">
                                    <div class="btn-group text-right">
                                        <button type="button" class="btn btn-success br2 btn-xs fs12 dropdown-toggle"
                                                data-toggle="dropdown" aria-expanded="false"> Active
                                            <span class="caret ml5"></span>
                                        </button>
                                        <ul class="dropdown-menu" role="menu">
                                            <li>
                                                <a href="#">Edit</a>
                                            </li>
                                            <li>
                                                <a href="#">Delete</a>
                                            </li>
                                            <li>
                                                <a href="#">Archive</a>
                                            </li>
                                            <li class="divider"></li>
                                            <li>
                                                <a href="#">Complete</a>
                                            </li>
                                            <li class="active">
                                                <a href="#">Pending</a>
                                            </li>
                                            <li>
                                                <a href="#">Canceled</a>
                                            </li>
                                        </ul>
                                    </div>
                                </td>
                            </tr>
                        {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        </body>
    </div>
</section>
{{> partials/footer.html}}
</html>
